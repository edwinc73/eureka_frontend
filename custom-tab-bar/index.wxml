<cover-view class="tab-bar {{width}} {{transition}}" >
  <cover-view wx:for="{{list}}" wx:key="index" class="tab-bar-item {{transition}} {{showIcon ? 'show' : ''}}" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
    <cover-image class="tab-bar-image"  animation="{{animationData}}" src="{{selected === index ? item.selectedIconPath : item.iconPath}}"></cover-image>
  </cover-view>
</cover-view>
