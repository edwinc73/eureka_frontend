.tab-bar{
  background: linear-gradient(153deg, #52BE8D 0%, #A1D97E 100%);
  height: 60px;
  border-radius: 500px;
  display: block;
  position: fixed;
  bottom: 32px;
  left: 32px ;
  right: 32px ;
  height: 60px;
  display: flex;
  padding: 0 29px;
  pointer-events: auto;
  position: fixed;
  z-index: 1000;
  box-sizing: border-box;
  box-shadow: 0px 4px 30px 0px rgba(0, 0, 0, 0.20);
}

/* custom-tab-bar/index.wxss */

.tab-bar-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tab-bar-item .tab-bar-image {
  width: 39px;
  height: 39px;
}

