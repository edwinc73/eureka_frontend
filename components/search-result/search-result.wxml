<view class="search-result-container">
  <view class="search-description">
  <image src="{{data.photos}}" mode="aspectFill" class="result-image"/>
  <view class="text-container">
    <view class="p-bold">{{data.name}}</view>
    <text class="small text-light">{{data.total_calories}}kcal</text>
  </view>
  </view>
  <image src="../../images/icons/goto.png" mode="aspectFit" class="go-to-icon" />
</view>

